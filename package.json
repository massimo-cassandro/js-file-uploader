{
  "name": "@massimo-cassandro/js-file-uploader",
  "version": "3.1.1",
  "description": "HTML5 Async File Uploader ",
  "main": "./js/file-uploader.js",
  "style": "./scss/fileUploader.scss",
  "module": "./dist/FileUploader.esm.js",
  "browser": "./dist/FileUploader.umd.js",
  "scripts": {
    "sass": "node_modules/.bin/sass ./scss/fileUploader.scss:./dist/fileUploader.css ./demo/css/fileuploader.scss:./demo/css/fileuploader.css --source-map --load-path=node_modules/ --style=compressed --watch",
    "rollup": "npx rollup --config --watch",
    "pug" : "node_modules/.bin/pug demo/pug --out demo/ --path demo/pug/tpl --pretty --watch",
    "echo_version": "echo $npm_package_version",
    "upd_version_var":"node upd_version.js && npx rollup --config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/js-file-uploader.git#main"
  },
  "author": "Massimo Cassandro",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ux",
    "user-interface",
    "user-experience",
    "forms",
    "upload-file",
    "javascript",
    "ajax",
    "interaction-design"
  ],
  "bugs": {
    "url": "https://github.com/massimo-cassandro/js-file-uploader.git/issues"
  },
  "homepage": "https://github.com/massimo-cassandro/js-file-uploader.git#readme",
  "files": [
    "dist/**/*.{css,js,map}",
    "js/**/*.{js,map}",
    "js-utilities/*.{js,map}",
    "scss/*.scss"
  ],
  "devDependencies": {
    "@massimo-cassandro/m-utilities": "^4.2.12",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "eslint": "^7.32.0",
    "js-beautify": "^1.14.0",
    "prismjs": "^1.24.1",
    "pug-cli": "^1.0.0-alpha6",
    "rollup": "^2.56.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.38.0",
    "stylelint": "^13.13.1",
    "stylelint-config-twbs-bootstrap": "^2.2.3"
  }
}
